        msg2 = await msg.answer(text.cre_project_exit, reply_markup=kb.exit_kb)
        
        if ("–í—ã—Ö–æ–¥ –≤ –º–µ–Ω—é2".lower() in msg.text.lower()) or ("/menu" in msg.text):
                    print(f"select_site –º—ã—Ö–æ–¥ –≤ –º–µ–Ω—é2")
